<div class="cartList">
    {{#each this.products as |product|}}
    <div class="cartItem">
        <img src="{{product.id.imageUrl}}" class="imageUrl">
        <div class="name">{{product.id.name}}</div>
        <div class="price">{{product.id.price}}</div>
        <div class="description">{{product.id.description}}</div>

        <button productId={{product._id}} class="addToCart">Edit</button>
    </div>
    {{/each}}
</div>